<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
        
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title + "Ohio Is For Sale" %></title>

    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <%= favicon_tag '/images/favicon.ico' %> 
    <%= stylesheet_link_tag "style" %>

    <link href="https://fonts.googleapis.com/css2?family=Roboto+Slab:wght@400;800&family=Roboto:wght@400;700&display=swap" rel="stylesheet">

    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-82875770-1', 'auto');
      ga("set", "page", location);
      ga("send", "pageview");
      ga("send", "event", "Scroll Pageview", location);

    </script>
    
  </head>
  
  <body class="<%= page_classes %>">
    <div class="outer-wrap">
      <%= partial "partials/nav-offcanvas" %>    
      <div class="inner-wrap">    
        <div id="header">
          <div class="page-wrap">
            <% link_to '/index.html' do %>
              <div id="logo">
                <h1>Ohio Is For Sale</h1>
              </div>
            <% end %>  
            <%= partial "partials/nav" %>
            <div id="hamburger">
                <div></div>
                <div></div>
                <div></div>
            </div>

          </div>  
         </div> 

          <div id="main" role="main">
            <div class="page-wrap">
              <div class="top"></div>
              <%= yield %>
              
            </div>
          </div>
          <%= partial "partials/footer" %>
        </div>
      </div>  
      <%= javascript_include_tag  "all" %>  
  </body>
</html>