<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <%= feed_tag :atom, "#{blog.options.prefix.to_s}/feed.xml", title: "Atom Feed" %>
        
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title + "Ohio Is For Sale" %></title>
    <link href='http://fonts.googleapis.com/css?family=Varela' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Montserrat:400,700' rel='stylesheet' type='text/css'>

    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <%= favicon_tag '../images/favicon.ico' %> 
    <%= stylesheet_link_tag "style" %>
    <%= javascript_include_tag  "all" %>
  </head>
  
  <body class="<%= page_classes %>">
    <div id="header">
      <div class="page-wrap">
        <% link_to '../../index.html' do %>
          <div id="logo"></div>
        <% end %>  
        <nav>
          <ul>
            <li><%= link_to 'Chapter Index','../chapterindex.html' %></li>
            <li><%= link_to 'Contact','../contact.html' %></li>
            <li><%= link_to 'Blog', 'http://expertlawncare.tumblr.com/', :target =>'_blank' %></li>
            <li><%= link_to 'Shop', 'http://ohioisforsale.bigcartel.com/', :target =>'_blank' %></li>

          </ul>
        </nav>
        <div id="hamburger">
            <div></div>
            <div></div>
            <div></div>
        </div>
        <nav id="dropdown">
          <ul>
            <li><%= link_to 'Chapter Index','../chapterindex.html' %></li>
            <li><%= link_to 'Contact','../contact.html' %></li>
            <li><%= link_to 'Blog', 'http://expertlawncare.tumblr.com/', :target =>'_blank' %></li>
            <li><%= link_to 'Shop', 'http://ohioisforsale.bigcartel.com/', :target =>'_blank' %></li>
          </ul>
        </nav>
      </div>  
     </div> 

      <div id="main" role="main">
      <%= yield %>
    </div>
  </body>
</html>